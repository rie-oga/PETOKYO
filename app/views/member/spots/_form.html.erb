
<%= form_with(model: [:member, spot], local: true) do |form| %>
  <%= form.attachment_field :spot_images_images, multiple: true %><!-- multiple: trueの記述で、複数画像選択可能になる -->
  <%= form.text_field :name, class: 'col-xs-12' %>
  <%= form.text_field :address, class: 'col-xs-12' %>
  <%= form.text_area :introduction, class: 'col-xs-12' %>
  <%= form.submit %>
<% end %>